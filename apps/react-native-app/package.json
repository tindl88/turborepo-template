{
  "name": "react-native-app",
  "version": "1.0.0",
  "private": true,
  "description": "React Native Template",
  "license": "MIT",
  "scripts": {
    "android:adb": "adb reverse tcp:3500 tcp:3500",
    "android": "node ./cli/copy.js && yarn android:adb && react-native run-android",
    "android:release": "yarn clean:android && node ./cli/copy.js && yarn android:adb && react-native run-android --variant=release",
    "ios": "node ./cli/copy.js && react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios:release": "node ./cli/copy.js && react-native run-ios --configuration Release --simulator='iPhone 15 Pro Max'",
    "ios:device": "node ./cli/copy.js && react-native run-ios --device='iPhone 11'",
    "ios:releasedevice": "node ./cli/copy.js && react-native run-ios --configuration Release --device='iPhone 11'",
    "codepush:android:staging": "appcenter codepush release-react -a tindl88/Bully-Android -d Staging",
    "codepush:android:stagingspecificversion": "appcenter codepush release-react -a tindl88/Bully-Android -c updateContents -t '*' -d Staging",
    "codepush:android:rollback": "appcenter codepush rollback -a tindl88/Bully-Android Staging --target-release v2",
    "codepush:android:history": "appcenter codepush deployment history -a tindl88/Bully-Android Staging",
    "codepush:ios:staging": "appcenter codepush release-react -a tindl88/Bully -d Staging",
    "codepush:ios:stagingspecificversion": "appcenter codepush release-react -a tindl88/Bully -c updateContents -t '*' -d Staging",
    "codepush:ios:stagingrange": "appcenter codepush release-react -a tindl88/Bully -t '*' -d Staging",
    "codepush:ios:rollback": "appcenter codepush rollback -a tindl88/Bully Staging --target-release v2",
    "codepush:ios:history": "appcenter codepush deployment history -a tindl88/Bully Staging",
    "codepush:staging": "yarn codepush:android:staging && yarn codepush:ios:staging",
    "codepush:stagingspecificversion": "yarn codepush:android:stagingspecificversion && yarn codepush:ios:stagingspecificversion",
    "codepush:history": "yarn codepush:android:history && yarn codepush:ios:history",
    "codepush:rollback": "yarn codepush:android:rollback && yarn codepush:ios:rollback",
    "start": "react-native start",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "vitest": "VITE_CJS_IGNORE_WARNING=true vitest --watch=false",
    "vitest:cov": "vitest run --coverage",
    "vitest:ui": "vitest --ui",
    "vitest:uicov": "vitest --ui --coverage.enabled",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "link:font": "npx react-native-asset",
    "commit": "better-commits",
    "gen:splash": "react-native generate-bootsplash src/assets/splash-logo.png --background=FFFFFF --logo-width=160 --assets-output=src/assets/bootsplash --flavor=main --html=index.html",
    "reset": "yarn clean:module && yarn clean:ios && yarn clean:android && yarn install && yarn reinstall",
    "reinstall": "yarn clean:pod && cd ios/ && pod install && cd ..",
    "clean:watcher": "watchman watch-del-all",
    "clean:cache": "yarn start --reset-cache",
    "clean:module": "rimraf node_modules",
    "clean:xcode": "rimraf ios/build ~/Library/Caches/CocoaPods ~/Library/Developer/Xcode/DerivedData/*",
    "clean:pod": "rimraf ios/Pods ios/Podfile.lock && pod cache clean --all",
    "clean:ios": "yarn clean:pod && yarn clean:xcode",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && rimraf android/build android/.gradle android/app/build",
    "clean:watchman": "watchman watch-del-all",
    "bump:major": "npm version major --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:buildnumber": "bundle exec fastlane bump_build_number",
    "postinstall": "patch-package",
    "prepare": "husky"
  },
