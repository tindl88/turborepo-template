{
  "prettier.useEditorConfig": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "files.exclude": {},
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "scss.lint.unknownAtRules": "ignore",
  "files.associations": {
    "*.html": "html",
    "*.njk": "nunjucks",
    "*.hbs": "handlebars",
    ".env": "bat",
    "__bit_reference": "cpp",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "__tree": "cpp",
    "array": "cpp",
    "deque": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "forward_list": "cpp",
    "initializer_list": "cpp",
    "ios": "cpp",
    "list": "cpp",
    "map": "cpp",
    "regex": "cpp",
    "set": "cpp",
    "span": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "valarray": "cpp",
    "vector": "cpp",
    "*.ipp": "cpp"
  },
  "java.compile.nullAnalysis.mode": "automatic",
  "java.configuration.updateBuildConfiguration": "disabled",
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  }
}
