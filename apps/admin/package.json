{
  "name": "admin",
  "version": "1.0.0",
  "private": true,
  "description": "NextJs - Admin Portal Template",
  "author": {
    "name": "Tin Tran",
    "email": "tindl88@gmail.com",
    "url": "https://github.com/tindl88"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "preview": "next build && next start",
    "test:watch": "vitest",
    "test": "vitest --watch=false",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:uicov": "vitest --ui --coverage.enabled",
    "test:e2e": "playwright test",
    "test:e2eui": "playwright test --ui",
    "deadcode": "ts-prune | wc -l",
    "commit": "better-commits",
    "postinstall": "patch-package",
    "prepare": "husky install && yarn genfont",
    "clean": "rimraf .next",
    "app:down": "docker-compose stop app",
    "app:log": "docker-compose logs -f app",
    "app:up": "docker-compose up -d app",
    "genfont": "rimraf src/libs/svg-icons/dist && node ./cli/generate-font.js"
  },
